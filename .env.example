
# Supabase - CONFIGURACIÓN PRINCIPAL
VITE_SUPABASE_URL=https://tu-proyecto.supabase.co
VITE_SUPABASE_ANON_KEY=tu_clave_publica_aqui

# Configuración de la App
VITE_APP_NAME=La-IA
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Sistema Operativo Inteligente para Restaurantes

# Configuración de desarrollo
VITE_ENABLE_DEBUG=false
VITE_MOCK_DATA=false

# Límites y configuraciones
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp,application/pdf
VITE_DEFAULT_TIMEZONE=Europe/Madrid

# Feature flags
VITE_FEATURE_AGENT_ENABLED=true
VITE_FEATURE_ANALYTICS_ENABLED=true
VITE_FEATURE_REALTIME_ENABLED=true

# Configuración adicional
VITE_DEFAULT_LANGUAGE=es
VITE_CURRENCY=EUR
VITE_DATE_FORMAT=DD/MM/YYYY
VITE_TIME_FORMAT=HH:mm

# Development
VITE_NODE_ENV=development
