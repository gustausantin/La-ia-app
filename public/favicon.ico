<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" width="512" height="512">
  <!-- Fondo degradado -->
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1d4ed8;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="text" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f0f9ff;stop-opacity:1" />
    </linearGradient>
    <!-- Filtro de sombra -->
    <filter id="shadow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="2" dy="4" stdDeviation="3" flood-color="#000000" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- Fondo del icono -->
  <rect width="512" height="512" rx="80" ry="80" fill="url(#bg)"/>
  
  <!-- CÃ­rculo decorativo -->
  <circle cx="256" cy="256" r="200" fill="none" stroke="rgba(255,255,255,0.1)" stroke-width="2"/>
  
  <!-- Logo La-IA -->
  <text x="256" y="200" font-family="Arial, sans-serif" font-size="80" font-weight="bold" 
        text-anchor="middle" fill="url(#text)" filter="url(#shadow)">La</text>
  
  <!-- IA con efecto especial -->
  <text x="256" y="280" font-family="Arial, sans-serif" font-size="60" font-weight="300" 
        text-anchor="middle" fill="url(#text)" filter="url(#shadow)">IA</text>
  
  <!-- Icono de restaurante (tenedor y cuchillo) -->
  <g transform="translate(256, 320)" fill="url(#text)" filter="url(#shadow)">
    <!-- Tenedor -->
    <path d="M-20,-10 L-20,30 M-25,-10 L-25,0 M-15,-10 L-15,0 M-20,0 L-20,-15" 
          stroke="url(#text)" stroke-width="3" fill="none" stroke-linecap="round"/>
    
    <!-- Cuchillo -->
    <path d="M20,-10 L20,30 M15,-10 L25,-5 L20,0" 
          stroke="url(#text)" stroke-width="3" fill="none" stroke-linecap="round"/>
    
    <!-- Plato -->
    <ellipse cx="0" cy="40" rx="30" ry="8" fill="none" stroke="url(#text)" stroke-width="2"/>
  </g>
  
  <!-- Efecto de brillo -->
  <path d="M 80,80 Q 180,120 280,80 Q 380,120 480,80 L 480,120 Q 380,160 280,120 Q 180,160 80,120 Z" 
        fill="rgba(255,255,255,0.1)"/>
</svg>
