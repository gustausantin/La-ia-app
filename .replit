run = "npm run dev"
modules = ["nodejs-20", "web"]
hidden = [".config", "package-lock.json"]
entrypoint = "src/index.jsx"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[nix]
channel = "stable-24_05"

[unitTest]
language = "nodejs"

[deployment]
run = ["sh", "-c", "npm run build && npm start"]
deploymentTarget = "cloudrun"
ignorePorts = false

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3001

[env]
XDG_CONFIG_HOME = "$REPL_HOME/.config"
PATH = "$REPL_HOME/.config/npm/node_global/bin:$REPL_HOME/node_modules/.bin"
npm_config_prefix = "$REPL_HOME/.config/npm/node_global"

[workflows]
runButton = "Development Stable"

[[workflows.workflow]]
name = "Start Development Server"
author = 41843571
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

[[workflows.workflow]]
name = "Dev with Host Fix"
author = 41843571
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "concurrently \"npm run server\" \"npx vite --host 0.0.0.0 --port 5173\""

[[workflows.workflow]]
name = "Fresh Dev Start"
author = 41843571
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "concurrently \"npm run server\" \"vite --host 0.0.0.0 --port 5173 --force\""

[[workflows.workflow]]
